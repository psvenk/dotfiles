snippet begin "\begin{} / \end{}" i
\begin{$1}
	$2
\end{$1}
endsnippet

snippet "tab(ular)?" "tabular" r
\begin{${1:tabular}}{$2}
	$3
\end{$1}
endsnippet

snippet " "TeX double quotes" i
\`\`$1''
endsnippet

snippet ` "TeX single quotes" i
\`$1'
endsnippet

snippet $ "TeX math mode" i
\$$1\$
endsnippet

snippet "v(ec)?" "Vector" ir
\vec{$1}
endsnippet
