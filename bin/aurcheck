#!/bin/sh
for dir in ~/src/aur/*
do
	pkg="$(echo "$dir" | sed 's|.*/src/aur/\(.*\)|\1|')"
	if [ "$pkg" = "mine" ]
	then
		continue
	fi
	cd "$dir" || continue

	git fetch
	if [ "$(git rev-parse HEAD)" != "$(git rev-parse '@{u}')" ]; then
		echo "$pkg"
	fi
	# https://stackoverflow.com/a/17938274
done
